{
  "name": "specht",
  "version": "1.1.0",
  "description": "Check links found in html or js files by pattern",
  "bin": "lib/cli.js",
  "main": "lib/runner.js",
  "license": "MIT",
  "author": {
    "name": "Eugene Datsky",
    "email": "eugene@datsky.ru"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "scripts": {
    "lint": "eslint src",
    "test": "ava test/**/*Spec.js --source=src/**/*.js --fail-fast",
    "build": "babel src --out-dir lib --source-maps",
    "prepublish": "npm run build"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/princed/specht.git"
  },
  "bugs": {
    "url": "https://github.com/princed/specht/issues"
  },
  "dependencies": {
    "babel-polyfill": "6.3.14",
    "babel-traverse": "6.4.5",
    "babel-types": "6.4.5",
    "babylon": "6.4.5",
    "ignore-file": "1.1.2",
    "parse5": "2.1.3",
    "readdirp": "2.0.0",
    "teamcity-service-messages": "0.1.6",
    "through2": "2.0.0",
    "yargs": "3.32.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "6.4.5",
    "babel-register": "6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.13.2",
    "eslint": "^3.3.1",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-plugin-import": "^1.14.0",
    "mock-fs": "^3.11.0",
    "sinon": "^1.17.5"
  }
}
